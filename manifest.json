{
    "manifest_version": 3,
    "name": "SustainableApparel",
    "description": "Be more mindful of the shops you patronize",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.ca/*",
                "https://www.amazon.com/*"
            ],
            "js": ["content.js"]
        }
    ]
}
